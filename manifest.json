{
  "manifest_version": 2,

  "name": "Force Auto-Complete",
  "description": "This extension allows the user to change the autocomplete property of a form field",
  "version": "1.0",

  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_start",
    "all_frames": true
  }],
  "icons": {
    "128": "icon.png"
  }
}